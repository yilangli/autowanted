    <servlet-name>Profile</servlet-name>
    <servlet-class>com.servlet.Profile</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Vehicle</servlet-name>
    <servlet-class>com.servlet.Vehicle</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Posts</servlet-name>
    <servlet-class>com.servlet.Posts</servlet-class>
  </servlet>
  <servlet>
